<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean
        class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
        <property name="location"
            value="classpath:org/cyclopsgroup/doorman/memory-hsql-db.properties" />
    </bean>
    <bean class="org.cyclopsgroup.doorman.api.SimpleUserSessionConfig" />
    <alias name="doormanTransactionManager" alias="transactionManager" />
    <import resource="classpath:org/cyclopsgroup/doorman/core-service.xml" />
</beans>